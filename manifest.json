{
    "manifest_version": 2,
    "name": "Working Hours Tracker",
    "description": "Working Hours Tracker",
    "version": "1.0",
    "browser_action": {
        "default_icon": "npm.png",
        "default_popup": "index.html"
    },
    "permissions": [],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "web_accessible_resources": [
        "assets/css/*",
        "assets/js/*",
        "assets/fonts/*"
    ]
}
